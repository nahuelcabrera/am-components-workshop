div
    span(ng-class='{ complete: $ctrl.item.complete }') {{ $ctrl.item.label }}
    button(type='button', ng-click='$ctrl.onChange({ $event: { todo: $ctrl.item } });') Done
    button(type='button', ng-click='$ctrl.onRemove({ $event: { todo: $ctrl.item } });') Delete
